-- Saber Simulator
local gameId = 3823781113
if game.PlaceId == gameId then
    print("Saber Simulator detectado")
    print("Carregando GG hub - Saber Simulator")

    local url = "https://raw.githubusercontent.com/DNzinBLOX/GGhub/main/GGhub_SaberSimulator.lua"
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if success then
        print("GG hub - Saber Simulator carregado com sucesso!")
    else
        warn("Erro ao carregar GG hub - Saber Simulator: " .. tostring(result))
    end
else
    print("Jogo n√£o suportado. Por favor, jogue em Saber Simulator (ID: " .. gameId .. ")")
end

